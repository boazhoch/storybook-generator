{
  "version": "0.1.7",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "story-cli-generator": "./dist/index.js"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "ts-node --esm ./src/index.ts",
    "build": "tsup",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "doc": "typedoc",
    "qa": "ts-node --esm ./src/index.ts -srcf './testlib/src/**/*.tsx' -ex './testlib/src/**/*.stories.tsx'",
    "qa2": "ts-node --esm ./src/index.ts -tsc './testlib/tsconfig.json'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "name": "story-cli-generator",
  "author": "Boaz Hoch",
  "module": "dist/story-cli-generator.esm.js",
  "size-limit": [
    {
      "path": "dist/story-cli-generator.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/story-cli-generator.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.1.0",
    "@tsconfig/node16": "^1.0.3",
    "@tsconfig/recommended": "^1.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.32.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.3",
    "size-limit": "^8.1.0",
    "ts-node": "^10.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "tsup": "^6.5.0",
    "typedoc": "^0.23.24",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "commander": "^9.5.0",
    "inversify": "5.0.5",
    "lodash": "^4.17.21",
    "pino": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "ts-morph": "^17.0.1",
    "uuid": "^9.0.0"
  }
}
